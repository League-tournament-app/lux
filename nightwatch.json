{
  "src_folders": ["./nightwatch/tests"],
  "test_settings": {
    "default": {
      "selenium_host": "ondemand.saucelabs.com",
      "selenium_port": 80,
      "username": "${SAUCELABS_USERNAME}",
      "access_key": "${SAUCELABS_ACCESS_KEY}",
      "launch_url": "http://localhost:8080",
      "silent": true,
      "desiredCapabilities": {
        "name": "tournamentsgg-chrome",
        "browserName": "chrome",
        "javascriptEnabled": true
      },
      "selenium": {
        "start_process": false
      }
    }
  }
}
