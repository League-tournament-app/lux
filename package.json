{
  "name": "@danpantry/lux",
  "version": "0.3.0",
  "description": "The React application for the League of Legends tournament app.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "test": "mocha --require babel/register src/**/*.spec.jsx --recursive",
    "e2e": "galen test ./e2e --recursive --htmlreport reports -DwebsiteUrl=http://${IP}:${PORT} -DseleniumGridUrl=http://${SAUCELABS_USERNAME}:${SAUCELABS_ACCESS_KEY}@ondemand.saucelabs.com:80/wd/hub",
    "clean": "rimraf lib/**/*.js",
    "prepublish": "babel --optional es7.decorators src/ --out-dir lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/League-tournament-app/lux.git"
  },
  "author": "Dan Pantry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/League-tournament-app/lux/issues"
  },
  "homepage": "https://github.com/League-tournament-app/lux#readme",
  "dependencies": {
    "radium": "^0.14.1",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-router": "1.0.0-rc1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "galenframework-cli": "^2.0.9",
    "git-sha-webpack-plugin": "0.0.4",
    "mocha": "^2.3.2",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  }
}
